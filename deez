local userName = game.Players.LocalPlayer.name
lcoal userId = game.Players.LocalPlayer.UserId
local allowed = false

local blacklisted = {
"Roblox"
"1"
}

if table.find(blacklisted, userName) then
	allowed = false
else
	allowed = true
end

if table.find(blacklisted, UserId) then
	allowed = false
else
	allowed = true
end

if allowed == false then
  print("failed")
  game.Players.LocalPlayer:Kick("bruh)
  
else
  return false
  print("passed")
end

